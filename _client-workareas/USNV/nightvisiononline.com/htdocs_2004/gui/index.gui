<script type="text/javascript">
	var name = getcookie("forumname");
	if (name == null) name = "";
	document.bbsform.name.value = name;
</script>
<script type="text/javascript">
	var email = getcookie("forumemail");
	if (email == null) email = "";
	document.bbsform.email.value = email;
</script>
<script type="text/javascript">
	var url = getcookie("forumlink");
	if (url == null) url = "";
	document.bbsform.url.value = url;
</script>

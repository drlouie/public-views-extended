	document.getElementById("GalleryName").innerHTML = ''+gallery+'';
	document.getElementById("GalleryCurrentAlbum").innerHTML = ''+caln+'';
	trigger('/fbqq.htm?PID='+tphid+'');
	showQQ();
	setAutoPlayResume();
}
function setCurrALN() {
	document.getElementById("GalleryName").innerHTML = ''+gallery+'';
	document.getElementById("GalleryCurrentAlbum").innerHTML = ''+caln+'';
//	alert(''+aln+'');
}



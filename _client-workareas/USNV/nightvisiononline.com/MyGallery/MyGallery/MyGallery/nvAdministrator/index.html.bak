	// for later use in viewing large photos already set to onClick photo inside SWF
}


// to read vars in flash and toggle on and off the AutoPlay for ssp [in essance a linked scriped to inner flash]
function setAutoPlayStop() {
	var sspMovie = document.ssp;
	sspMovie.SetVariable("_root.stopTheAP", "1");
}
function setAutoPlayResume() {
	var sspMovie = document.ssp;
	sspMovie.SetVariable("_root.stopTheAP", "0");
}
function setDynLoad() {
	var sspMovie = document.ssp;
	sspMovie.SetVariable("_root.dynLoad", "1");
	sspMovie.SetVariable("_root.DLalbum", "USNV_Moderators_Colleczioni");
	sspMovie.SetVariable("_root.DLphoto", "5");
}

var theCB = 0;
var countT = 0;
function trigger(myurl) {
	var myurl = myurl;
	if (myurl.indexOf("?") != -1) {
		if (countT != 0) { theCB = '&CB=' + countT + ''; }
		frame.loadpage(''+myurl+'&'+theCB+'');
		countT++;
	}
	else {
		frame.loadpage(''+myurl+'');
	}
}

function setCurrPH(tphid,telph,taln,tcaln) {
//	alert(tphid+''+telph+''+taln+''+tcaln);
	phid = tphid;
	elph = telph;
	aln = taln;
	caln = tcaln;

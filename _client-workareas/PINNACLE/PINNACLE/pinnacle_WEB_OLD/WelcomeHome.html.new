<script>

var cual = '';
var vis = '';
var sop = '';
var fop = '';
var currOpacity = '';
function slowFadeOut(cual) {
	cual = cual;
	fader(cual,"hide","59","0","60");
}
function slowFadeIn(cual) {
//	cual = cual;
//	fader(cual,"show","01","60","00");
}

function fader(cual, vis, sop, fop, currOpacity) {

	currOpacity = Number(currOpacity);
	if (vis == 'show') {
		if (fop > currOpacity) { 
			newOpacity = Number(currOpacity + 1);
			currOpacity = newOpacity;
		}
	}
	else if (vis == 'hide') {
		if (fop < currOpacity) { 
			newOpacity = Number(currOpacity - 1);
			currOpacity = newOpacity;
		}

	}


	myLayer = document.getElementById(""+cual+"").style;
 	if (myLayer.MozOpacity)
		myLayer.MozOpacity="." + currOpacity + "";
	else if (myLayer.filter)
   		myLayer.filter = "Alpha(Opacity=" + currOpacity + ");";

	//alert("" + cual + "," + vis + "," + sop + "," + fop + "," + currOpacity + "");
	window.setTimeout("fader('" + cual + "','" + sop + "','" + fop + "','" + vis + "','" + currOpacity + "')", 10);



}
</script>
